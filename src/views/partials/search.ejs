<div class="B-categoria">
  <form action="/Evento/buscar" method="GET" class="buscador2">
    <input
      type="text"
      name="event_name"
      placeholder="Buscar un evento en Deporteando"
    />
    <select name="locations" id="locations">
      <option value="">localidad</option>
      <!-- Aplicar el For -->
      <% for (event of events){ %>
      <option value="<%=event.location.id%>">
        <%= event.location.locations %>
      </option>
      <% } %>
    </select>
    <select name="event_date" id="mes">
      <option value="">Por Mes.</option>
      <option value="2021-08-01">Ago de 2021</option>
      <option value="2021-09-01">Sep de 2021</option>
      <option value="2021-10-01">Oct de 2021</option>
      <option value="2021-11-01">Nov de 2021</option>
      <option value="2021-12-01">Dic de 2021</option>
      <option value="2022-01-01">Ene de 2022</option>
      <option value="2022-02-01">Feb de 2022</option>
      <option value="2022-03-01">Mar de 2022</option>
      <option value="2022-04-01">Abr de 2022</option>
      <option value="2022-05-01">May de 2022</option>
      <option value="2022-06-01">Jun de 2022</option>
      <option value="2023-03-19">Jul de 2022</option>
      <option value="2022-06-27">Jul de 2027</option>
    </select>
    <button class="categori-buttom" type="submit">BUSCAR</button>
  </form>
</div>
